{
    "env": {
        "browser": true,
        "node": true,
        "jest": true
    },
    "extends": "airbnb",
    "rules": {
        "strict": 0,
        "semi": ["error", "never"],
        "import/prefer-default-export": ["off"],
        "no-use-before-define": ["error", { "functions": false }],
        "prefer-destructuring": ["error", {
          "array": true,
          "object": true
        }]
      },
    "parser": "babel-eslint"
}