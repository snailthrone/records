{
  "name": "records",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build:client": "parcel build --detailed-report --log-level 3 --no-cache --out-dir public/js --out-file bundle.js src/Index.jsx",
    "build:server": "babel src/ -d build/",
    "build": "yarn build:server && yarn build:client",
    "lint": "eslint --ext .js,.jsx src/",
    "start": "node build/Server.jsx",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "yarn watch:server && yarn watch:client",
    "watch:client": "parcel watch --log-level 3 --public-url \"http://localhost:8080/\" --out-dir public/js --out-file bundle.js src/Index.jsx",
    "watch:server": "nodemon --exec babel-node src/Server.jsx"
  },
  "keywords": [],
  "author": "Eemeli Martti (@snailthrone)",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.0",
    "nodemon": "^1.18.3"
  },
  "engines": {
    "node": "10.9.0"
  },
  "main": "Index.js",
  "repository": "git@github.com:snailthrone/records.git",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-polyfill": "^6.26.0",
    "express": "^4.16.3",
    "prop-types": "^15.6.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "styled-components": "^3.2.6"
  }
}
